<html>
  <head>
    <title>{{html_title}}</title>
    <link rel="stylesheet" href="../../results/static/css/style.css">
  </head>
  <body>
    <section class="main">
      <h1>{{html_title}}</h1>
      {%for content in set_data %}
        {% if content.flag == "img"%}
          <h2>{{ content.title }}</h2>
          <img class="main__content--image" src="{{content.img_src}}" />
          <a href="{{content.img_src}}">Data Link Raw Image</a>
          <p>{{ content.comment }}</p>
        {% elif content.flag == "dataframe" %}
          <h2>{{content.title}} </h2>
          <p>{{content.comment}}</p>
        <div class ="table">
          {{ content.dataframe.to_html()|safe}}
        </div>
        {% elif content.flag =="to_text"%}
        <div>
          <p>{{content.comment}}</p>
        </div>
        {% elif content.flag =="to_list_text"%}
        <div class ="table">
          {% for unpack_list in content.text_data %}
            <table>

            {% for list_text in unpack_list %}
            <tr>
              {% for actual_text in list_text %}
                <td>{{actual_text}}</td>
              {%endfor%}
            </tr>

            {% endfor %}
          </table>

          {%endfor%}
        </div>

        {%endif%}
      {%endfor%}
    </section>
  </body>
</html>