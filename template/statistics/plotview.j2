<html>
  <head>
    <title>{{html_title}}</title>
    <link rel="stylesheet" href="../../results/static/css/style.css">
  </head>
  <body>
    <section class="main">
      <h1>{{html_title}}</h1>
      {%for content in set_data %}
          <h2>{{ content.title }}</h2>
          <img class="main__content--image" src="{{content.img_src}}" />
          <a href="{{content.img_src}}">Data Link Raw Image</a>
          <p>{{ content.comment }}</p>
      {%endfor%}
    </section>
  </body>
</html>